<div class="signees above-fold">

    <figure class="top-fig">
        <img src="/img/clark-young-135152.jpg" alt="farm pic">
    </figure>
    <h2>We, residents of {{cityName}}, pledge to eat local!</h2>

    <!-- [{"first_name":"Leonardo","last_name":"DiCaprio"},{"first_name":"Maggie","last_name":"Wiseman"}]
    -->
    <ul>
        {{#each results}}
        <li>{{#if this.homepage}}
            <a href="{{this.homepage}}">{{this.first_name}} {{this.last_name}} </a>
            {{else}}
                {{this.first_name}} {{this.last_name}}
            {{/if}}
            {{#if this.age}}
                 ({{this.age}})
            {{/if}}
        </li>
        {{/each}}
    </li>
</div>
